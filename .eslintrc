{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
        "document": true,
        "window": true
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "babel"
    ],
    "rules": {
        "babel/semi": 1,
        "react/prop-types": 0
    }
}